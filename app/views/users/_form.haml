- form_for @user do |f|
  = f.error_messages
  %p
    Create the Administrator account:
  %p
    =f.label :first_name
    =f.text_field :first_name
    
  %p
    =f.label :last_name
    =f.text_field :last_name
    
  %p
    =f.label :email
    =f.text_field :email
    
  
  %p
    = f.label :username
    = f.text_field :username
  
  %p
    = f.label :password
    = f.password_field :password
  
  
  %p
    = f.label :password_confirmation
    = f.password_field :password_confirmation
  
  = f.submit "Submit"
